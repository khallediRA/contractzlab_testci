    "build": "tsc --build",
    "makemigration": "npm run build && makemigration --models-path .\\js\\models",
    "runmigration": "npm run build && runmigration --models-path .\\js\\models",
    "generate-interfaces": "node js/scripts/generateInterfaces.js",
    "generate-vapid-keys": "npx web-push generate-vapid-keys",
    "start": "npm run runmigration && node --max-old-space-size=8192 js/server.js",
    "dev": "tsc --build && node js/server.js --max-old-space-size=8192",
    "test": "echo \"Error: no test specified\" && exit 1"
